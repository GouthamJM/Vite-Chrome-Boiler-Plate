{
    "manifest_version": 3,
    "name": "CRXJS React Vite Example",
    "version": "1.0.0",
    "action": { "default_popup": "src/index.html" },
    "content_scripts": [
        {
          "js": ["src/pages/content/index.ts"],
          "matches": ["http://*/*", "https://*/*"],
          "run_at": "document_start",
          "all_frames": true
        }
      ],
      "web_accessible_resources": [
        {
            "matches": ["<all_urls>"],
            "resources": [
                "injector.js"
            ],
            "use_dynamic_url": true
        }
    ]
    
  }